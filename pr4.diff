+            {dueDateStatus === 'warning' && '‚è∞ '}
+            {formatDate(props.data.dueDate)}
+          </Badge>
+        ) : (
+          <span className="text-muted">No due date</span>
+        )}
+        {showDatePicker ? (
+          <Form.Control
+            type="date"
+            size="sm"
+            style={{ width: '150px', display: 'inline-block' }}
+            onChange={handleDateChange}
+            onBlur={() => setShowDatePicker(false)}
+            autoFocus
+          />
+        ) : (
+          <Button
